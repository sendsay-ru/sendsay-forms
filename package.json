{
  "name": "sendsay-forms",
  "version": "0.0.33",
  "description": "Library for displaying popups",
  "keywords": [
    "popup"
  ],
  "bugs": {
    "url": "https://github.com/sendsay-ru/sendsay-forms"
  },
  "repository": {
